<?xml version="1.0" encoding="UTF-8"?>
<mappings xmlns="http://dozermapper.github.io/schema/bean-mapping"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://dozermapper.github.io/schema/bean-mapping http://dozermapper.github.io/schema/bean-mapping.xsd">

    <!--全局配置，配置日期的映射格式-->
    <configuration>
        <date-format>yyyy-MM-dd HH:mm:ss</date-format>
    </configuration>

    <!--淼信创建运单数据对象映射-->
    <mapping wildcard="true">
        <class-a>com.brandslink.cloud.logistics.entity.centre.BaseOrder</class-a>
        <class-b>com.brandslink.cloud.logistics.thirdLogistics.bean.MiaoXin.MiaoXinOrder</class-b>
        <field>
            <a>logisticsMethodCode</a>
            <b>product_id</b>
        </field>
        <field>
            <a>customerOrderNumber</a>
            <b>order_customerinvoicecode</b>
        </field>
        <!--收件人信息-->
        <field>
            <a>consigneeCountryCode</a>
            <b>country</b>
        </field>
        <field>
            <a>consigneeFirstName</a>
            <b>Consignee_name</b>
        </field>
        <field>
            <a>consigneeStreet</a>
            <b>consignee_address</b>
        </field>
        <field>
            <a>consigneeCity</a>
            <b>consignee_city</b>
        </field>
        <field>
            <a>consigneeState</a>
            <b>consignee_state</b>
        </field>
        <field>
            <a>consigneeZip</a>
            <b>consignee_postcode</b>
        </field>
        <field>
            <a>consigneePhone</a>
            <b>consignee_mobile</b>
        </field>
        <field>
            <a>consigneePhone</a>
            <b>consignee_telephone</b>
        </field>
        <!--发件人信息-->
        <field>
            <a>consignorCountryCode</a>
            <b>shipper_country</b>
        </field>
        <field>
            <a>consignorFirstName</a>
            <b>shipper_name</b>
        </field>
        <field>
            <a>consignorCompany</a>
            <b>shipper_companyname</b>
        </field>
        <field>
            <a>consignorStreet</a>
            <b>shipper_address1</b>
        </field>
        <field>
            <a>consignorCity</a>
            <b>shipper_city</b>
        </field>
        <field>
            <a>consignorState</a>
            <b>shipper_state</b>
        </field>
        <field>
            <a>consignorZip</a>
            <b>shipper_postcode</b>
        </field>
        <field>
            <a>consignorPhone</a>
            <b>shipper_telephone</b>
        </field>
        <!--子对象集合映射-->
        <field>
            <!--两个字段-->
            <a>childs</a>
            <b>orderInvoiceParam</b>
            <!--两个字段对应的实体类-->
            <a-hint>com.brandslink.cloud.logistics.entity.centre.BaseOrderChild</a-hint>
            <b-hint>com.brandslink.cloud.logistics.thirdLogistics.bean.MiaoXin.OrderInvoice</b-hint>
        </field>
    </mapping>
    <mapping>
        <class-a>com.brandslink.cloud.logistics.entity.centre.BaseOrderChild</class-a>
        <class-b>com.brandslink.cloud.logistics.thirdLogistics.bean.MiaoXin.OrderInvoice</class-b>
        <field>
            <a>hsCode</a>
            <b>hs_code</b>
        </field>
        <field>
            <a>quantity</a>
            <b>invoice_pcs</b>
        </field>
        <field>
            <a>enName</a>
            <b>invoice_title</b>
        </field>
        <field>
            <a>unitWeight</a>
            <b>invoice_weight</b>
        </field>
        <field>
            <a>sku</a>
            <b>sku</b>
        </field>
        <field>
            <a>unitPrice</a>
            <b>unitPrice</b>
        </field>
    </mapping>

    <!--顺友创建运单数据对象映射-->
