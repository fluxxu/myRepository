        <!--子对象集合映射-->
        <field>
            <!--两个字段-->
            <a>childs</a>
            <b>productList</b>
            <!--两个字段对应的实体类-->
            <a-hint>com.brandslink.cloud.logistics.entity.centre.BaseOrderChild</a-hint>
            <b-hint>com.brandslink.cloud.logistics.thirdLogistics.bean.SunYou.SunYouProduct</b-hint>
        </field>
    </mapping>
    <mapping>
        <class-a>com.brandslink.cloud.logistics.entity.centre.BaseOrderChild</class-a>
        <class-b>com.brandslink.cloud.logistics.thirdLogistics.bean.SunYou.SunYouProduct</class-b>
        <field>
